{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "./src",
    "module": "commonjs",
    "lib": [
      "dom",
      "dom.iterable",
      "es6",
      "esnext"
    ],
    "outDir": "dist",
    "allowJs": true,
    "skipLibCheck": true,
    "declaration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
